{
  "cli": {
    "version": ">= 15.0.15",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug",
        "credentialsSource": "remote"
      },
      "prebuildCommand": "npx expo prebuild --no-install"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      },
      "env": {
        "GOOGLE_SERVICES_FILE": "./google-services.json"
      },
      "prebuildCommand": "npx expo prebuild --no-install"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle",
        "credentialsSource": "remote"
      },
      "env": {
        "GOOGLE_SERVICES_FILE": "./google-services.json"
      },
      "prebuildCommand": "npx expo prebuild --no-install"
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
